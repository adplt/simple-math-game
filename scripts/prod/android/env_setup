# Env variables for building android app

# Set production build
export PRODUCTION_BUILD="true"

# Version code - used by playstore to determine the latest version
# Should be bumped up every release
export VERSION_CODE=500100

# Version name - this is displayed to the user
# Change the version name every release
export VERSION_NAME="5.1.0"

# Java keystore to be used to sign the app
export KEYSTORE=/Users/adityasaxena/Documents/mckinsey/2017/sinarmas/mobile-app/android/app/SimasKeystore.jks

# Alias for the key store
export ALIAS_KS="SimobiPlus"

# Application ID
export APP_ID="com.simas.mobile.SimobiPlus"

# AAPT path
export AAPT=/Users/adityasaxena/Downloads/android_sdk/build-tools/23.0.1/aapt

# SPLUNK MINT production key
export MINT_KEY="e4d30357"
